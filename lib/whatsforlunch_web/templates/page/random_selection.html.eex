<%= form_for @conn, page_path(@conn, :random_range), fn f -> %>

  <div class="form-group">
    <%= label f, "Sure! How many options would you like?", class: "control-label" %>
    <%= number_input f, :count, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= submit "Submit", class: "btn btn-primary" %>
  </div>
<% end %>
<span><%= link "Back", to: page_path(@conn, :index) %></span>
